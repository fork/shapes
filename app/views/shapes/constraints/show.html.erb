<%= navigation %>
<% if @struct_primitive %>
<ul><%= render :partial => '/shapes/constraints/struct_constraint', :collection => @resource.constraints %></ul>
<%= link_to_add_struct_constraint @struct_primitive, @resource %>
<% else %>
<ul><%= render :partial => '/shapes/constraints/constraint', :collection => @resource.constraints %></ul>
<%= link_to_add_constraint @shape, @resource %>
<% end %>
